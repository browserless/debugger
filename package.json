{
  "name": "browserless-debugger",
  "version": "1.0.2",
  "description": "The web-client for browserless/chrome debugging",
  "main": "static/index.js",
  "files": [
    "static/*"
  ],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build-dev": "node ./node_modules/webpack/bin/webpack.js",
    "build": "node ./node_modules/webpack/bin/webpack.js --optimization-minimize --mode production",
    "dev": "npm run build-dev -- --watch",
    "serve": "npm run build && http-server -a localhost ./static"
  },
  "keywords": [
    "browserless",
    "debugger",
    "puppeteer"
  ],
  "author": "joelgriffith",
  "license": "GPL-3.0-or-later OR Browserless Commercial License",
  "devDependencies": {
    "@types/file-saver": "^2.0.3",
    "buffer": "^6.0.3",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.4.0",
    "file-loader": "^6.1.0",
    "file-saver": "^2.0.5",
    "file-type": "^16.5.3",
    "html-webpack-plugin": "^5.3.2",
    "jszip": "^3.10.1",
    "monaco-editor": "^0.29.1",
    "monaco-editor-webpack-plugin": "^5.0.0",
    "puppeteer-core": "^8.0.0",
    "raw-loader": "^4.0.2",
    "style-loader": "^3.3.0",
    "ts-loader": "^9.2.6",
    "typescript": "^4.4.4",
    "webpack": "^5.58.2",
    "webpack-cli": "^4.9.0",
    "webpack-dev-server": "^4.3.1"
  },
  "dependencies": {
    "http-server": "^14.1.1"
  }
}
